{
  "name": "the-darkest-lib",
  "version": "1.0.0",
  "description": "A spcific extension of Phaser game engine.",
  "main": "lib/TDLib.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "docs": "./node_modules/.bin/jsdoc -d ./docs -t ./node_modules/ink-docstrap/template -R README.md -r ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FrancescoNegri/TheDarkestLib.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs",
    "phaser"
  ],
  "author": "Francesco Negri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FrancescoNegri/TheDarkestLib/issues"
  },
  "homepage": "https://github.com/FrancescoNegri/TheDarkestLib",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "eslint": "^5.15.1",
    "eslint-loader": "^2.0.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "jsdom": "13.2.0",
    "jsdom-global": "3.0.2",
    "mocha": "^6.0.2",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.0.8",
    "yargs": "^13.2.2"
  },
  "dependencies": {}
}
